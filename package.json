  "bugs": {
    "url": "https://github.com/leesan70/nodejs_exercise_with_nodejs/issues"
  },
  "homepage": "https://github.com/leesan70/nodejs_exercise_with_nodejs#readme",
  "dependencies": {
    "body-parser": "^1.19.0",
    "express": "^4.17.1",
    "mysql2": "^1.6.5",
    "sequelize": "^5.15.1",
    "sequelize-cli": "^5.4.0"
  },
  "devDependencies": {
    "mocha": "^6.1.4",
    "should": "^13.2.3",
    "supertest": "^4.0.2"
  }
}
